{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "test": "jest --watch",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "gen": "graphql get-schema && graphql-schema-typescript generate-ts schema.graphql --output ./src/graphql-types.d.ts "
  },
  "dependencies": {
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.18",
    "@types/react-helmet": "^5.0.7",
    "@types/styled-components": "^4.1.4",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.0.76",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.17",
    "graphql-schema-typescript": "^1.2.8",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.4",
    "typeface-clear-sans": "^0.0.44",
    "typeface-oswald": "^0.0.54",
    "typescript": "^3.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "graphql-cli": "^3.0.3",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0"
  }
}
